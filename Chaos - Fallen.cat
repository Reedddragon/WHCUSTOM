        <profile name="Weapon Master" typeId="9cc3-6d83-4dd3-9b64" typeName="Abilities" hidden="false" id="1287-2932-f1ab-3546">
          <characteristics>
            <characteristic name="Description" typeId="9b8f-694b-e5e-b573">At start of command phase, this model can switch weapons from Lucifer &amp; Sin or Lucifer &amp; Abyss</characteristic>
