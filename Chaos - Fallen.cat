        </profile>
        <profile name="Invulnerable Save" typeId="9cc3-6d83-4dd3-9b64" typeName="Abilities" hidden="false" id="5c18-f813-5b50-d98b">
          <characteristics>
            <characteristic name="Description" typeId="9b8f-694b-e5e-b573">This model has a 4+ invulnerable save.</characteristic>
          </characteristics>
        </profile>
        <profile name="Red Horseman, War" typeId="796e-cfd7-cc85-240d" typeName="Knight Devil of The Apocalyse" hidden="false" id="ea3b-204e-fc7d-e555">
          <characteristics>
